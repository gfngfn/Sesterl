module TestString = struct

  let phrase() =
    'Hello World!'

  let chop_first(s) =
    case s of
    | []         -> None
    | ch :: tail -> Some({ch, tail})
    end

  let starts_with_h(s) =
    case s of
    | $'H' :: _ -> true
    | _         -> false
    end

  let main(args) =
    let s = phrase() in
    let ans1 = chop_first(s) in
    let ans2 = $'F' :: 'oo' in
    let ans3 = starts_with_h(s) in
    print_debug({s, ans1, ans2, ans3})

end
